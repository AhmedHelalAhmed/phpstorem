<templateSet group="Laravel-Blade">
  <template name="@for" value="@for ($i = 0; $i &lt; $LIMIT$; $i++)&#10;   $END$&#10;@endfor" description="Blade @for" toReformat="false" toShortenFQNames="true">
    <variable name="LIMIT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@foreach" value="@foreach ($USERS$ as $USER$)&#10;    $END$&#10;@endforeach" description="Blade @foreach" toReformat="false" toShortenFQNames="true">
    <variable name="USERS" expression="" defaultValue="&quot;$users&quot;" alwaysStopAt="true" />
    <variable name="USER" expression="" defaultValue="&quot;$user&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@include" value="@include('$VIEW$'$DATA$)" description="Blade @include" toReformat="false" toShortenFQNames="true">
    <variable name="VIEW" expression="" defaultValue="&quot;partials.filename&quot;" alwaysStopAt="true" />
    <variable name="DATA" expression="" defaultValue="&quot;, ['data' =&gt; $data]&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@layout" value="@extends('layouts.default')&#10;&#10;@section('sidebar')&#10;    @parent&#10;    &#10;@stop&#10;&#10;@section('content')&#10;&#9;@include('forms.create')&#10;&#9;&#10;@stop" description="Blade template skeleton" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@section" value="@section('$SECTION$')&#10;    $END$&#10;@$STOP$" description="Blade @section" toReformat="false" toShortenFQNames="true">
    <variable name="SECTION" expression="" defaultValue="&quot;content&quot;" alwaysStopAt="true" />
    <variable name="STOP" expression="" defaultValue="&quot;stop&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@yield" value="@yield('$CONTENT$'$DEFAULT$)$END$" description="Blade @yield" toReformat="false" toShortenFQNames="true">
    <variable name="CONTENT" expression="" defaultValue="&quot;content&quot;" alwaysStopAt="true" />
    <variable name="DEFAULT" expression="" defaultValue="&quot;, 'Default Content'&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@extends" value="@extends('layouts.$NAME$')$END$" description="Blade @extends" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;default&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="@forelse" value="@foreach ($USERS$ as $USER$)&#10;    $END$&#10;@forelse&#10;&#10;@endforelse" description="Blade @forelse" toReformat="false" toShortenFQNames="true">
    <variable name="USERS" expression="" defaultValue="&quot;$users&quot;" alwaysStopAt="true" />
    <variable name="USER" expression="" defaultValue="&quot;$user&quot;" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
    </context>
  </template>
</templateSet>